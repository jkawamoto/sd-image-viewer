{
  "name": "frontend",
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "upgrade": "ncu -u && yarn",
    "generate": "js-yaml -c ../openapi.yaml > openapi.json && swagger-typescript-api -p openapi.json -o ./src && rm openapi.json"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/carousel": "^6.0.19",
    "@mantine/core": "^6.0.19",
    "@mantine/dates": "^6.0.19",
    "@mantine/hooks": "^6.0.19",
    "dayjs": "^1.11.9",
    "embla-carousel-react": "^8.0.0-rc12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "js-yaml": "^4.1.0",
    "npm-check-updates": "^16.13.1",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.2"
  },
  "license": "MIT",
  "private": true
}
